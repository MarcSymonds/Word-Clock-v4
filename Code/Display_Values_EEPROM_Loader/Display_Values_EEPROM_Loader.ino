void setup() {
  Serial.begin(115200);
  
  int i = 0;
  int j = 1;
  boolean eof = false;
  byte d;
  
  //Serial.print(j);
  //Serial.print("=");
  Serial.println(0);
  
  while (!eof)
  {
    d = data[i];
    
    if (d == 255)
    {
      if (data[i + 1] == 255)
        eof = true;
      else
      {
        ++j;
        //Serial.print(j);
        //Serial.print("=");
        
        Serial.println(i+1);
      }
    }
    
    EEPROM.update(i, d);
    
    ++i;
  }
}

